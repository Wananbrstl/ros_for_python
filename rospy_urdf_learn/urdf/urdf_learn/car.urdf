<robot name = "myCar">
    <link name = "base_footprint" >
        <visual>
            <geometry>
                <sphere radius = "0.001" />
                <origin xyz = "0 0 0" rpy = "0 0 0" />
            </geometry>
        </visual>
    </link>
    <link name = "base">
        <visual>
            <geometry>
                <cylinder radius = "0.1" length = "0.08" />
                <origin xyz = "0 0 0" rpy = "0 0 0" />
            </geometry>
        </visual>
    </link>


    <joint name = "link2footprint" type = "fixed">
        <parent link = "base_footprint" />
        <child link = "base" />
        <origin xyz = "0 0 0.055" rpy = "0 0 0" />
    </joint> 


    <link name = "left_wheel">
        <visual>
            <geometry>
                <cylinder radius = "0.0325" length = "0.015" />
                <origin xyz = "0 0 0" rpy = "1.5708 0 0 " />
            </geometry>
        </visual>
    </link>
    <link name = "right_wheel">
        <visual>
            <geometry>
                <cylinder radius = "0.0325" length = "0.015" />
                <origin xyz = "0 0 0" rpy = "1.5708 0 0  " />
            </geometry>
        </visual>
    </link>

    <joint name = "left2base" type = "continuous">
        <parent link = "base" />
        <child link = "left_wheel" />
        <origin xyz = "0 0.1 -0.0225" rpy = "1.5708 0 0 " />
        <axis xyz = "0  0 1 " />
    </joint>

    <joint name = "right2base" type = "continuous">
        <parent link = "base" />
        <child link = "right_wheel" />
        <origin xyz = "0 -0.1 -0.0225" rpy = "1.5708 0 0 "  />
        <axis xyz = "0  0 1 " />
    </joint>
    <!-- 万向轮 -->
    <link name = "uni_wheel">
        <visual>
            <geometry>
                <sphere radius = "0.0075" />
                <origin xyz = "0 0 0.00" rpy = "0 0 0 " />
            </geometry>
        </visual>
    </link>

    <joint name = "basetowanxiang" type = "continuous"> 
        <parent link = "base" />
        <child link = "uni_wheel" />
        <origin xyz = " 0.08 0 -0.0475" rpy = "0 0 0" />
        <axis xyz = "1 1 1" />
    </joint>

    <link name = "uni_wheel2">
        <visual>
            <geometry>
                <sphere radius = "0.0075" />
                <origin xyz = "0 0 0.00" rpy = "0 0 0 " />
            </geometry>
        </visual>
    </link>

    <joint name = "basetowanxiang2" type = "continuous"> 
        <parent link = "base" />
        <child link = "uni_wheel2" />
        <origin xyz = " -0.08 0 -0.0475" rpy = "0 0 0" />
        <axis xyz = "1 1 1" />
    </joint>

</robot>